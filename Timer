    print("59")
    time.sleep(1)
    print("60")
    time.sleep(1)

def main():
    # Repeat the counting process 10 times
    repeat_times = 10
    for _ in range(repeat_times):
        repeat_count()
        print("Completed one cycle of counting.\n")

# Call the main function to start the repeating process
main()
